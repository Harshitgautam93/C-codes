//WAP for Multiplication Table of a number
#include <iostream>

int main() {
    // Declare variables
    int number;

    // Get input from user
    std::cout << "Enter a number to display its multiplication table: ";
    std::cin >> number;

    // Display multiplication table
    std::cout << "Multiplication table for " << number << ":\n";
    for (int i = 1; i <= 10; ++i) {
        std::cout << number << " x " << i << " = " << (number * i) << "\n";
    }

    return 0;
}
