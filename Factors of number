#include <iostream>

int main() {
    // Declare variables
    int number;

    // Get input from user
    std::cout << "Enter a positive integer: ";
    std::cin >> number;

    // Display factors of the number
    std::cout << "Factors of " << number << " are: ";
    for (int i = 1; i <= number; ++i) {
        if (number % i == 0) {
            std::cout << i << " ";
        }
    }

    return 0;
}
